void pieChart(float diameter, float[] data, int centerx, int centery, color[] colorset) {
  float lastAngle = 0;
  for (int i = 0; i < data.length; i++) {
    fill(colorset[i]);
    arc(centerx, centery, diameter, diameter, lastAngle, lastAngle+radians(data[i]));
    lastAngle += radians(data[i]);
  }
